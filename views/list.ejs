<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%=Title%></h1>
    </div>

    <div class="box">
        <% for(var i=0;i<taskList.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%=taskList[i]%></p>
            </div>
        <% } %>
        <form class="item" action="/" method="post">
            <input type="text" name="taskinput" placeholder="Enter Your Task!" autocomplete="off">
            <button type="submit" name="tasksubmit" value="<%=Title%>">+</button>
        </form>
    </div>
<%- include("footer") -%>